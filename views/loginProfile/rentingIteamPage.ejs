<%- include('../partials/loginHeader') %>


<div class="profile-div" >
        <h2 id="profile-div-heading">Hello, <%=accountName%></h2>
        <h3>Items, You can take it on Rent.</h3>


        <% for(var i=0;i<iteamsObj.length;i++){ %>


          <div class="profile-div-product-box" id="profile-div-product-box<%=i%>">
               <img src="<%=iteamsObj[i].imgfile%>" alt="">


               <div class="profile-div-product-box-details">
                   <h3><%=iteamsObj[i].name%></h3>


                   <div class="profile-div-product-box-details-discription">
                      <p><%=iteamsObj[i].discription%></p>
                   </div>


                   <h4>Rent - <%=iteamsObj[i].city%></h4>
                   <p>â‚¹ <%=iteamsObj[i].price%>/day</p>

                  <input class="hide" type="text" name="iteamId" value="<%=iteamsObj[i]._id%>">
                  <input class="hide profile-div-city" type="text" name="city" value="<%=iteamsObj[i].city%>">
                  <input class="hide profile-div-category" type="text" name="category" value="<%=iteamsObj[i].category%>">
                  <button onclick="addToCart('<%=iteamsObj[i]._id%>','<%=iteamsObj[i].price%>');"  class="uploaded-add-to-cart" type="submit">Add to Cart</button>

             </div>
          </div>





        <%} %>





</div>

<script src="script/loginItemsMenu.js" charset="utf-8"></script>
<%- include('../partials/footer.ejs') %>
